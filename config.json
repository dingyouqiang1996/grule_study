    "EbsSsdGenericPerMonthCostRule": {
      "rule_conditions": [
        "EBSCost.BillMode == 1",
        "EBSCost.SyshdType == \"SSD-generic\""
      ],
      "rule_desc": "通用型SSD磁盘(SSD-generic) 包月计费规则",
      "rule_logics": [
        "SSD_GENERIC = 0.7",
        "EBSCost.Cost = SSD_GENERIC * EBSCost.InstanceCnt * EBSCost.CycleCount",
        "Retract(\"EbsSsdGenericPerMonthCostRule\")"
      ],
      "rule_salience": "10"
    },
    "EbsSsdPerMonthCostRule": {
      "rule_conditions": [
        "EBSCost.BillMode == 1",
        "EBSCost.SyshdType == \"SSD\""
      ],
      "rule_desc": "超高IO(SSD) 包月计费规则",
      "rule_logics": [
        "SSD = 1.2",
        "EBSCost.Cost = SSD * EBSCost.InstanceCnt * EBSCost.CycleCount",
        "Retract(\"EbsSsdPerMonthCostRule\")"
      ],
      "rule_salience": "10"
    }
